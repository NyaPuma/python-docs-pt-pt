"static inline void _Py_SET_TYPE(PyObject *ob, PyTypeObject *type)\n"
"{ ob->ob_type = type; }\n"
"#define Py_SET_TYPE(ob, type) _Py_SET_TYPE((PyObject*)(ob), type)\n"
"#endif"

